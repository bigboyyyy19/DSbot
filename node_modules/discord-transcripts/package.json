{
  "name": "discord-transcripts",
  "version": "3.6.4",
  "description": "A nicely formatted html transcript generator for discord.js.",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "postbuild": "cp ./src/template.html ./dist/template.html",
    "test:commonjs": "node ./tests/generate.js",
    "test:typescript": "ts-node ./tests/generate.ts"
  },
  "keywords": [
    "discord.js",
    "discord.js-transcripts",
    "discord.js-transcripts",
    "html-transcripts",
    "discord-html-transcripts"
  ],
  "author": "cozy",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "discord.js": "^13.6.0",
    "he": "^1.2.0",
    "highlight.js": "^11.4.0",
    "jsdom": "^19.0.0"
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.3",
    "@types/he": "^1.1.2",
    "@types/jsdom": "^16.2.14",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
